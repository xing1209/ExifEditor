            <property name="styleSheet">
             <string notr="true">QTextBrowser {
    /* 背景颜色 */
    background-color: rgb(76, 76, 76);

    /* 边框样式：虚线 + 颜色 */
    border: 1px dashed rgb(187, 187, 187);

    /* 字体颜色 */
    color: white;

    /* 字体大小 */
    font-size: 14px;

    /* 圆角边框（可选） */
    border-radius: 8px;

    /* 内边距 */
    padding: 8px; /* 调整文本内容与边框的距离 */
}

QScrollBar:vertical, QScrollBar:horizontal {
    /* 滑动条背景颜色 */
    background-color: rgb(76, 76, 76);

    /* 边框 */
    border: 1px solid rgb(187, 187, 187);

    /* 宽度（垂直）或高度（水平） */
    width: 12px;
    height: 12px;
}

QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    /* 滑块颜色 */
    background-color: rgb(187, 187, 187);

    /* 圆角滑块 */
    border-radius: 6px;

    /* 滑块内边距 */
    min-height: 20px;
    min-width: 20px;
}

QScrollBar::add-line, QScrollBar::sub-line {
    /* 按钮背景 */
    background-color: rgb(76, 76, 76);

    /* 按钮颜色 */
    border: 1px solid rgb(187, 187, 187);

    /* 圆角按钮 */
    border-radius: 6px;

    /* 高度（上下按钮）或宽度（左右按钮） */
    height: 12px;
    width: 12px;

    /* 不显示文字 */
    subcontrol-position: none;
}

QScrollBar::add-line:hover, QScrollBar::sub-line:hover, QScrollBar::handle:hover {
    /* 鼠标悬停时颜色变化 */
    background-color: rgb(255, 255, 255);
}

QScrollBar::add-page, QScrollBar::sub-page {
    /* 空白区域的背景颜色 */
    background-color: rgb(76, 76, 76);
}
</string>
